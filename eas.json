{
  "build": {
    "production": {
      "releaseChannel": "production",
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "apk"
      }
    },
    "test": {
      "releaseChannel": "test",
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_RESET_DB_ON_INIT": "false",
        "EXPO_PUBLIC_STORYBOOK_ENABLED": "false"
      }
    }
  },
  "cli": {
    "version": ">=16.2.2",
    "appVersionSource": "local"
  }
}
